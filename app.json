{
  "expo": {
    "name": "VOYA_App",
    "slug": "VOYA_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "voyaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "react": {
      "strictMode": false
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow VOYA_App to access your camera",
        "NSMicrophoneUsageDescription": "Allow VOYA_App to access your microphone"
      }
    },
    "android": {
    "package": "com.nmnghi.VOYA_App",
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/logo.png",
      "backgroundColor": "#FFFFFF"
      },


      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-vision-camera",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "ndk": {
              "abiFilters": [
                "armeabi-v7a",
                "arm64-v8a",
                "x86",
                "x86_64"
              ]
            }
          }
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c1192456-53d7-4aa8-b783-5f29ed7fc62c"
      }
    },
    "owner": "center1806"
  }
}
